URL: @EPUB_PROJECT_URL@
Version: @EPUB_VERSION@
CFlags: -I${includedir} @EPUB_PKG_CONFIG_DEFINES@ @EPUB_PKG_CFLAGS@
Libs: -L${libdir} -l@LIBRARY@
